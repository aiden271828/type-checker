{
  "name": "@aiden-organization/runtime-type-check-test",
  "version": "0.0.2",
  "description": "It allows you to check a type with Typescript syntax at javascript runtime.",
  "keywords": [
    "type",
    "check",
    "verify",
    "TypeScript"
  ],
  "author": "Aiden",
  "license": "MIT",
  "repository": "https://github.com/aiden271828/runtime-type-check",
  "homepage": "https://github.com/aiden271828/runtime-type-check",
  "bugs": "https://github.com/aiden271828/runtime-type-check/issues",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "rollup --config rollup.config.ts --configPlugin typescript --watch",
    "build": "rimraf lib && rollup --config rollup.config.ts --configPlugin typescript && npm run api",
    "lint": "eslint . --ext .ts,.js --fix",
    "api": "rimraf temp && tsc src/index.ts --declaration --emitDeclarationOnly --declarationMap --outDir temp && api-extractor run --local --verbose && rimraf temp"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.29.5",
    "@rollup/plugin-typescript": "^8.4.0",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "tslib": "^2.4.0",
    "typescript": "~4.7.4",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "unpkg": "lib/index.js",
  "jsdelivr": "lib/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ]
}